{
	"vertex_shaders": [
		{
			"basic": [
				"attribute vec3 inVertexPosition;",
				"void main(void) {",
				"gl_Position = vec4(inVertexPosition, 1.0);",
				"}"
			]
		}
	],

	"fragment_shaders": [
		{
			"basic": [
				"precision mediump float;",
				"void main(void) {",
				"gl_FragColor = vec4(1.0, 1.0, 1.0, 1.0);",
				"}"
			]
		}
	]
}